    <string name="mus_folkart_phone">"+30 231 088 9840"</string>
    <!-- Info, Museums, Thessaloniki Olympic Museum -->
    <string name="mus_olympic_title">Thessaloniki Olympic Museum</string>
    <string name="mus_olympic_description">"Thessaloniki Olympic Museum, the unique Olympic Museum of Greece, is situated at Thessaloniki, Central Macedonia, Greece. The museum is located on the confluence and educational, athletic and cultural routes of the city."</string>
    <string name="mus_olympic_address">"3is Septemvriou, Thessaloniki 546 36"</string>
    <string name="mus_olympic_hours">"Mon-Fri: 9AM–7PM\nSat: 10AM–4PM\nSun: 10AM–2PM"</string>
    <string name="mus_olympic_phone">"+30 231 096 8531"</string>

